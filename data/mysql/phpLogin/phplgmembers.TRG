TYPE=TRIGGERS
triggers='CREATE DEFINER=`` TRIGGER move_to_deleted_members AFTER\n                                                  DELETE ON phplgmembers FOR EACH ROW\n                                                  BEGIN\n                                                    DELETE FROM phplgdeleted_members\n                                                      WHERE phplgdeleted_members.id = OLD.id;\n                                                    INSERT INTO phplgdeleted_members (id, username, password, email, verified)\n                                                    VALUES ( OLD.id, OLD.username, OLD.password, OLD.email, OLD.verified );\n                                                  END' 'CREATE DEFINER=`` TRIGGER assign_default_role AFTER\n                                                INSERT ON phplgmembers FOR EACH ROW\n                                                BEGIN\n                                                  SET @default_role = (SELECT id FROM phplgroles WHERE default_role = 1 LIMIT 1);\n                                                  INSERT INTO phplgmember_roles (member_id, role_id) VALUES (NEW.id, @default_role);\n                                                END' 'CREATE DEFINER=`` TRIGGER prevent_deletion_of_superadmin\n                                                            BEFORE DELETE\n                                                            ON phplgmembers\n                                                            FOR EACH ROW\n                                                            BEGIN\n                                                            	IF\n                                                              (SELECT count(m.id)\n                                                            	FROM phplgmembers m\n                                                            	INNER JOIN phplgmember_roles mr on mr.member_id = m.id\n                                                            	INNER JOIN phplgroles r on mr.role_id = r.id\n                                                            	WHERE\n                                                            	 m.verified = 1\n                                                            	AND m.banned = 0\n                                                            	AND r.name = \'Superadmin\'\n                                                                AND m.id = OLD.id) > 0\n                                                              THEN\n                                                                SIGNAL SQLSTATE \'45000\' SET MESSAGE_TEXT = \'Cannot delete superadmin user\';\n                                                              END IF;\n                                                            END'
